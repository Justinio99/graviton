<?xml version="1.0" encoding="UTF-8" ?>
<routes xmlns="http://symfony.com/schema/routing">
  <route id="graviton_corebundle_App_all" pattern="/app">
    <default key="_controller">gravitoncorebundle.controller.app:allAction</default>
    <requirement key="_method">GET</requirement>
  </route>
  <route id="graviton_corebundle_App_get" pattern="/app/{id}">
    <default key="_controller">gravitoncorebundle.controller.app:getAction</default>
    <requirement key="_method">GET</requirement>
  </route>
  <route id="graviton_corebundle_App_post" pattern="/app">
    <default key="_controller">gravitoncorebundle.controller.app:postAction</default>
    <requirement key="_method">POST</requirement>
  </route>
  <route id="graviton_corebundle_App_put" pattern="/app/{id}">
    <default key="_controller">gravitoncorebundle.controller.app:putAction</default>
    <requirement key="_method">PUT</requirement>
  </route>
</routes>
